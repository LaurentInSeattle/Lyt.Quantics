<UserControl 
	x:Class="Lyt.Quantics.Studio.Workflow.Run.Gates.GatesView"
	xmlns="https://github.com/avaloniaui"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d" d:DesignWidth="320" d:DesignHeight="850"
    xmlns:vm="clr-namespace:Lyt.Quantics.Studio.Workflow.Run.Gates"
    x:DataType="vm:GatesViewModel" 
    xmlns:theme="clr-namespace:Lyt.Quantics.Studio"
	Width="320" 
	VerticalAlignment="Stretch"
	
	DragDrop.AllowDrop="True"
	>
	<Grid
		ShowGridLines="False"
		x:Name="MainGrid"
		DragDrop.AllowDrop="True"
		>
		<Grid.RowDefinitions>
			<RowDefinition Height="80" />
			<RowDefinition Height="80" />
			<RowDefinition Height="80" />
			<RowDefinition Height="80" />
			<RowDefinition Height="80" />
			<RowDefinition Height="80" />
			<RowDefinition Height="*" />
		</Grid.RowDefinitions>
		<Grid.ColumnDefinitions>
			<ColumnDefinition Width="80" />
			<ColumnDefinition Width="80" />
			<ColumnDefinition Width="80" />
			<ColumnDefinition Width="80" />
		</Grid.ColumnDefinitions>
	</Grid>
</UserControl>
