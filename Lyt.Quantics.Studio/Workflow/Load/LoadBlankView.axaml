<UserControl 
    x:Class="Lyt.Quantics.Studio.Workflow.Load.LoadBlankView"
	xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
	>
	<Grid
		Margin="16"
		>
		<TextBlock
			Margin="100 0 0 0"
			Text="Creates an empty quantum computer circuit with two qubits initialized at the ground state zero."
			VerticalAlignment="Top" HorizontalAlignment="Left"
			TextAlignment="Left"
			FontSize="18"
			FontStyle="Italic"
			TextWrapping="WrapWithOverflow"
			Foreground="{StaticResource ResourceKey= LightAqua_0_120}"
			Opacity="0.95"
			/>
	</Grid>
</UserControl>
