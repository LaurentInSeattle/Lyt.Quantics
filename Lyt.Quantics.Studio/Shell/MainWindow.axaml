<Window 
    x:Class="Lyt.Quantics.Studio.Shell.MainWindow"
	xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d" d:DesignWidth="1080" d:DesignHeight="640"
    Icon="/Assets/Images/ket.ico"
    Title="Quantics Studio"
	Width="1680" Height="980"
	MinWidth="400" MinHeight="360"
	SizeToContent="Manual"
	WindowStartupLocation="CenterScreen"
	>
	<Grid
		x:Name="MainWindowGrid"
		>
		<Image
            Stretch="UniformToFill"
            Source="\Assets\Images\dark.jpg"
            Grid.ColumnSpan="999" Grid.RowSpan="999"
            />
		<Rectangle
			Fill="#20000020"
            Grid.ColumnSpan="999" Grid.RowSpan="999"
			/>
		<Canvas 
			x:Name="MainWindowCanvas"
			Background="Transparent"
			IsHitTestVisible="false"
			ZIndex="999"
			DragDrop.AllowDrop="True"
			/>
	</Grid>
</Window>
